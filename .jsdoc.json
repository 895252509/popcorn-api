{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": "./",
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|build|docs|views|gulpfile*)"
  },
  "plugins": [
    "plugins/markdown",
    "node_modules/jsdoc-babel"
  ],
  "opts": {
    "template": "node_modules/docdash",
    "encoding": "utf8",
    "destination": "docs/",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  }
}
