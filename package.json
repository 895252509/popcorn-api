    "docs": "./node_modules/.bin/esdoc -c .esdoc.json",
    "lint": "./node_modules/.bin/eslint -c .eslintrc.js src/**/*.js"
  },
  "bin": {
    "popcorn-api": "./build/popcorn-api.js"
  },
  "engines": {
    "node": ">=6.3.0"
  },
  "main": "./build/popcorn-api.js",
