{
  "name": "popcorn-api",
  "version": "2.2.4",
  "description": "Popcorn Time API is developed to make it easier for anyone to create their own version of Popcorn Time.",
  "dependencies": {
    "async-q": "^0.3.1",
    "body-parser": "^1.17.2",
    "butter-provider": "git+https://github.com/butterproviders/butter-provider.git",
    "bytes": "^2.5.0",
    "commander": "^2.11.0",
    "compression": "^1.7.0",
    "cron": "^1.2.1",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "express-winston": "^2.4.0",
    "extratorrent-api": "^1.1.4",
    "eztv-api-pt": "^1.0.12",
    "fanart.tv-api": "^1.0.3",
    "horriblesubs-api": "^1.0.6",
    "kat-api-pt": "^2.0.1",
    "mongoose": "^4.11.5",
    "node-tvdb": "^3.2.0",
    "nyaa-api-pt": "^1.0.9",
    "omdb-api-pt": "^1.0.1",
    "parse-torrent": "^5.8.3",
    "prompt": "^1.0.0",
    "response-time": "^2.3.2",
    "sprintf-js": "^1.1.1",
    "tmdbapi": "^1.0.2",
    "trakt.tv": "^5.1.0",
    "webtorrent-health": "^1.1.1",
    "winston": "^2.3.1",
    "yts-api-pt": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-dynamic-import-node": "^1.0.2",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-regenerator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-register": "^6.24.1",
    "chai": "^4.1.0",
    "chai-http": "^3.0.0",
    "coveralls": "^2.13.1",
    "cross-env": "^5.0.2",
    "del": "^3.0.0",
    "dredd": "^4.1.2",
    "esdoc": "^1.0.1",
    "esdoc-accessor-plugin": "1.0.0",
    "esdoc-brand-plugin": "1.0.0",
    "esdoc-coverage-plugin": "1.0.0",
    "esdoc-ecmascript-proposal-plugin": "1.0.0",
    "esdoc-external-ecmascript-plugin": "1.0.0",
    "esdoc-external-nodejs-plugin": "1.0.0",
    "esdoc-flow-plugin": "^1.0.0",
    "esdoc-flow-type-plugin": "1.0.0",
    "esdoc-integrate-manual-plugin": "1.0.0",
    "esdoc-integrate-test-plugin": "1.0.0",
    "esdoc-lint-plugin": "1.0.0",
    "esdoc-publish-html-plugin": "1.0.0",
    "esdoc-undocumented-identifier-plugin": "1.0.0",
    "esdoc-unexported-identifier-plugin": "1.0.0",
    "eslint": "^4.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "flow": "^0.2.3",
    "flow-bin": "^0.51.1",
    "flow-typed": "^2.1.5",
    "forever": "^0.15.3",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "mocha": "^3.5.0",
    "nyc": "^11.1.0"
  },
  "license": "MIT",
  "scripts": {
    "build": "gulp build",
    "coveralls": "cat ./coverage/lcov.info | node ./node_modules/.bin/coveralls",
    "dev": "cross-env NODE_ENV=development babel-node ./src/popcorn-api.js -m pretty",
    "docs": "esdoc -c .esdoc.json",
    "flow": "flow",
    "flow-typed": "flow-typed",
    "forever": "cross-env NODE_ENV=prod forever start ./build/popcorn-api.js -m pretty",
    "lint": "eslint -c .eslintrc.js {src,test}/** ./*.js",
    "prestart": "npm run build",
    "pretest": "npm run build",
    "providers": "cross-env NODE_ENV=production babel-node ./src/scraper/configs",
    "start": "cross-env NODE_ENV=prod node ./build/popcorn-api.js -m pretty",
    "test": "npm run test-dredd && npm run test-mocha",
    "test-dredd": "cross-env NODE_ENV=test babel-node ./node_modules/dredd/bin/dredd",
    "test-mocha": "cross-env NODE_ENV=test ./node_modules/.bin/nyc --reporter=lcov --reporter=text ./node_modules/.bin/mocha --recursive --timeout 10000 --reporter spec test/**"
  },
  "bin": {
    "popcorn-api": "./build/popcorn-api.js"
  },
  "engines": {
    "node": ">=6.3.0"
  },
  "main": "./build/popcorn-api.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/popcorn-official/popcorn-api.git"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
