{
  "name": "popcorn-api",
  "version": "2.2.4",
  "description": "Popcorn Time API is developed to make it easier for anyone to create their own version of Popcorn Time.",
  "dependencies": {
    "body-parser": "^1.18.2",
    "butter-provider": "git+https://github.com/butterproviders/butter-provider.git",
    "bytes": "^3.0.0",
    "commander": "^2.11.0",
    "compression": "^1.7.1",
    "cron": "^1.3.0",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "express-winston": "^2.4.0",
    "eztv-api-pt": "^1.3.1",
    "fanart.tv-api": "^2.0.1",
    "fs-extra": "^4.0.2",
    "horriblesubs-api": "^1.0.7",
    "kat-api-pt": "^3.0.0",
    "mongoose": "^4.12.3",
    "node-tvdb": "^3.2.0",
    "nyaa-api-pt": "^2.0.0",
    "omdb-api-pt": "^2.0.1",
    "p-times": "^1.0.1",
    "parse-torrent": "^5.8.3",
    "prompt": "^1.0.0",
    "response-time": "^2.3.2",
    "sprintf-js": "^1.1.1",
    "tmdbapi": "^1.0.2",
    "trakt.tv": "^6.0.1",
    "webtorrent-health": "^1.1.1",
    "winston": "^3.0.0-rc0",
    "yts-api-pt": "^2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-plugin-dynamic-import-node": "^1.1.0",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-strict-mode": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "coveralls": "^3.0.0",
    "cqc": "^0.4.0",
    "cross-env": "^5.1.0",
    "del": "^3.0.0",
    "dredd": "^4.5.0",
    "esdoc": "^1.0.3",
    "esdoc-accessor-plugin": "1.0.0",
    "esdoc-brand-plugin": "1.0.0",
    "esdoc-coverage-plugin": "1.1.0",
    "esdoc-ecmascript-proposal-plugin": "1.0.0",
    "esdoc-external-ecmascript-plugin": "1.0.0",
    "esdoc-external-nodejs-plugin": "1.0.0",
    "esdoc-flow-plugin": "^1.0.0",
    "esdoc-flow-type-plugin": "1.0.1",
    "esdoc-integrate-manual-plugin": "1.0.0",
    "esdoc-integrate-test-plugin": "1.0.0",
    "esdoc-lint-plugin": "1.0.1",
    "esdoc-publish-html-plugin": "1.1.0",
    "esdoc-undocumented-identifier-plugin": "1.0.0",
    "esdoc-unexported-identifier-plugin": "1.0.0",
    "eslint": "^4.9.0",
    "eslint-config-vixo": "^2.0.0",
    "flow": "^0.2.3",
    "flow-bin": "^0.57.3",
    "flow-typed": "^2.2.0",
    "forever": "^0.15.3",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.0",
    "husky": "^0.14.3",
    "mocha": "^4.0.1",
    "nyc": "^11.2.1",
    "sinon": "^4.0.1"
  },
  "license": "MIT",
  "scripts": {
    "build": "gulp build",
    "coveralls": "cat ./coverage/lcov.info | node ./node_modules/.bin/coveralls",
    "cqc": "./node_modules/.bin/cqc \"{src,test}/**/*.js\" *.js --verbose",
    "coverage": "./node_modules/.bin/babel-node coverage.js",
    "debug": "cross-evn NODE_ENV=development DEBUG=popcorn-api:* node --require babel-register --inspect-brk=http://localhost:9229 ./src/popcorn-api.js -m pretty",
    "dev": "cross-env NODE_ENV=development ./node_modules/.bin/babel-node ./src/popcorn-api.js -m pretty",
    "docs": "./node_modules/.bin/esdoc -c .esdoc.json",
    "flow": "./node_modules/.bin/flow",
    "forever": "cross-env NODE_ENV=prod ./node_modules/.bin/forever start ./build/popcorn-api.js -m pretty",
    "lint": "./node_modules/.bin/eslint {src,test}/** ./*.js",
    "precommit": "npm run lint && npm run flow",
    "preforever": "npm run build",
    "prepush": "npm run test",
    "prestart": "npm run build",
    "pretest": "npm run build",
    "providers": "./node_modules/.bin/babel-node ./src/popcorn-api.js --providers",
    "start": "cross-env NODE_ENV=production node ./build/popcorn-api.js -m pretty",
    "test": "npm run test-dredd && npm run test-mocha",
    "test-dredd": "cross-env NODE_ENV=test ./node_modules/.bin/babel-node ./node_modules/dredd/bin/dredd",
    "test-mocha": "cross-env NODE_ENV=test ./node_modules/.bin/nyc --reporter=lcov --reporter=text ./node_modules/.bin/mocha --recursive --timeout 30000 --exit --reporter spec test/**"
  },
  "bin": {
    "popcorn-api": "./build/popcorn-api.js"
  },
  "engines": {
    "node": ">=6.3.0"
  },
  "main": "./build/popcorn-api.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/popcorn-official/popcorn-api.git"
  },
  "eslintConfig": {
    "extends": "vixo",
    "globals": {
      "logger": true
    }
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
